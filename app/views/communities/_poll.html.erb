<div class="community-poll">
  <h4>
    <%= link_to poll.title, proposal_poll_path(@community.proposal, poll) %>
  </h4>

  <p class="topic-info">
    <%= t("communities.poll.take_part",
           from: l(poll.starts_at.to_date), to: l(poll.ends_at.to_date)) %>
  </p>

   <script>
      /**
      * Override class for MFP-POLICY
      * @author Panuwat Chaiwongthon
      */
      if(window.location.pathname.includes('communities')){
        $('.community-poll a').click(() => {
        sessionStorage.setItem('path-forward', window.location.pathname + window.location.hash)
        })
      }
  </script>
</div>
